{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "7.3.*",
        "ext-json": "*",
        "arrilot/laravel-widgets": "^3.13",
        "aws/aws-sdk-php-laravel": "^3.3",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "barryvdh/laravel-ide-helper": "^2.8",
        "doctrine/dbal": "2.9.*",
        "dompdf/dompdf": "^0.8.5",
        "fideloper/proxy": "^4.0",
        "hulkur/laravel-hasmany-keyby": "^1.1",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "lcobucci/jwt": "3.3",
        "league/csv": "^9.6",
        "makeabledk/laravel-factory-enhanced": "^2.1",
        "milon/barcode": "^5.1",
        "sentry/sentry-laravel": "^1.3",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-activitylog": "3.9.1",
        "spatie/laravel-permission": "^3.13",
        "spatie/laravel-query-builder": "^2.8",
        "spatie/laravel-tags": "^2.5.0",
        "thomasjohnkane/snooze": "^1.0",
      "ext-simplexml": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "jasonmccreary/laravel-test-assertions": "^1.0",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "mpociot/laravel-test-factory-helper": "1.*",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "install-hooks": [
            "rm -rf .git/hooks",
            "ln -s ../scripts/hooks .git/hooks"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "pre-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@install-hooks"
        ],
        "post-install-cmd": [
            "@php artisan app:install"
        ]
    }
}
