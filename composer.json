{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "7.3.*",
        "ext-json": "*",
        "arrilot/laravel-widgets": "^3.13",
        "aws/aws-sdk-php-laravel": "^3.6",
        "barryvdh/laravel-dompdf": "^0.9",
        "barryvdh/laravel-ide-helper": "^2.8",
        "doctrine/dbal": "^3.0",
        "dompdf/dompdf": "^0.8.5",
        "fideloper/proxy": "^4.4",
        "hulkur/laravel-hasmany-keyby": "^1.1",
        "laravel/framework": "^6.20",
        "laravel/passport": "^9.3",
        "laravel/tinker": "^2.5",
        "lcobucci/jwt": "3.3",
        "league/csv": "^9.6",
        "makeabledk/laravel-factory-enhanced": "^2.1",
        "milon/barcode": "^6.0",
        "sentry/sentry-laravel": "^2.4",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-permission": "^4.0",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-tags": "^4.0",
        "thomasjohnkane/snooze": "^1.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "jasonmccreary/laravel-test-assertions": "^1.1",
        "laravel/dusk": "^6.13",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "mpociot/laravel-test-factory-helper": "^2.1",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^1.16.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "install-hooks": [
            "rm -rf .git/hooks",
            "ln -s ../scripts/hooks .git/hooks"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "pre-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@install-hooks"
        ],
        "post-install-cmd": [
            "@php artisan app:install"
        ]
    }
}
