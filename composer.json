{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "8.1.*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "arrilot/laravel-widgets": "3.13.*",
        "aws/aws-sdk-php-laravel": "3.6.*",
        "barryvdh/laravel-dompdf": "^0.9",
        "blackfire/php-sdk": "^1.31",
        "doctrine/dbal": "^2.13",
        "dompdf/dompdf": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "grayloon/laravel-magento-api": "^0.12.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "hulkur/laravel-hasmany-keyby": "^4.0.2",
        "laravel/forge-sdk": "3.*",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "lcobucci/jwt": "^4.2",
        "league/csv": "^9.6",
        "makeabledk/laravel-factory-enhanced": "^2.1|3.*|4.*",
        "milon/barcode": "^9.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "romanzipp/laravel-queue-monitor": "2.1.*",
        "sentry/sentry-laravel": "^2.4",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-activitylog": "^4.1",
        "spatie/laravel-database-mail-templates": "^3.4",
        "spatie/laravel-permission": "^5.0",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-ray": "^1.17",
        "spatie/laravel-tags": "^4.3.5",
        "thomasjohnkane/snooze": "^1.0",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "2.8.*",
        "beyondcode/laravel-query-detector": "^1.6",
        "bpocallaghan/generators": "^7.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "knuckleswtf/scribe": "^3.23",
        "laravel/dusk": "^6.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "install-hooks": [
            "rm -rf .git/hooks",
            "ln -s ../scripts/hooks .git/hooks"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "pre-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@install-hooks"
        ],
        "post-install-cmd": [],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
